 index="6" value="300"/><column index="7" value="84"/><column index="8" value="63"/><column index="9" value="65"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="Chinook2.sql">-- first we gonig to asking about
-- top 5 artist has albums ? 
SELECT artists.Name , count(albums.ArtistId) &quot;Number of Album&quot;
FROM albums
JOIN artists on albums.ArtistId=artists.ArtistId
GROUP by artists.Name
ORDER by &quot;Number of Album&quot; DESC 
LIMIT 7;

-- second question
-- Who are the top buying customer for music ?
SELECT (customers.FirstName || &quot; &quot; || customers.LastName) as CustomerName ,
 sum(invoices.Total) as total
FROM invoices
INNER JOIN customers on invoices.CustomerId = customers.CustomerId
GROUP by CustomerName 
ORDER by total DESC
LIMIT 7;

-- Third Question 
-- who's the top media type format used?
SELECT media_types.Name , count(tracks.MediaTypeId) as total
FROM tracks
JOIN media_types on tracks.MediaTypeId = media_types.MediaTypeId
GROUP by media_types.Name
ORDER by total DESC
LIMIT 5;


-- fourth question 
-- who is the most supportive employee ?

SELECT (employees.FirstName || &quot; &quot; || employees.LastName) as EmployeeName ,
 count(customers.CustomerId) as &quot;Number of Customer&quot;
FROM employees
JOIN customers on employees.EmployeeId = customers.SupportRepId
GROUP by EmployeeName; 


-- fifthe question
-- what is the most Country  buying music ?

SELECT customers.Country , count(invoices.BillingCountry) &quot;Most Countery billing&quot;
FROM invoices
JOIN customers on invoices.BillingCountry = customers.Country
GROUP by customers.Country 
ORDER by &quot;Most Countery billing&quot; DESC
LIMIT 7;

-- Sixth question
-- what is the most city  buying music ?


SELECT customers.City , count(invoices.BillingCity) &quot;Most City billing&quot;
FROM invoices
JOIN customers on invoices.BillingCity = customers.City
GROUP by customers.City 
ORDER by &quot;Most City billing&quot; ;


-- Seventh question
-- what is the most  state buying music ?

SELECT customers.State, count(invoices.BillingState) &quot;Most State billing&quot;
FROM invoices
JOIN customers on invoices.BillingState = customers.State
GROUP by customers.State 
ORDER by &quot;Most State billing&quot; DESC;


-- Eighth question 
-- who is the most gener id ?

SELECT tracks.GenreId, count(genres.GenreId) &quot;Most GENER&quot;
FROM genres
JOIN tracks on genres.GenreId = tracks.GenreId
GROUP by genres.GenreId 
ORDER by &quot;Most GENER&quot; DESC;








